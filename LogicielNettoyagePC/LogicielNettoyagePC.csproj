<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="clean.png" />
    <None Remove="historique.png" />
    <None Remove="sweeping.png" />
    <None Remove="update.png" />
    <None Remove="www.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="clean.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="historique.png" />
    <Resource Include="sweeping.png" />
    <Resource Include="update.png" />
    <Resource Include="www.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
